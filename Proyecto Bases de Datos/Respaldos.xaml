        <Button x:Name="btn_menuP" Content="Menu" HorizontalAlignment="Left" Margin="710,347,0,0" VerticalAlignment="Top" Width="59" Background="White" Height="36" Click="btn_menuS_Click"/>
        <TabControl HorizontalAlignment="Left" Height="285" Margin="32,47,0,0" VerticalAlignment="Top" Width="664">
            <TabItem Header="Respaldos">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Administración archivos">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Recuperación">
